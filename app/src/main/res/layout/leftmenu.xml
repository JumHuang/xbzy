<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:background="#ffffffff"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	
    <LinearLayout
		android:gravity="center" 
		android:orientation="vertical" 
		android:background="#ff4fc3f7"
		android:layout_width="fill_parent"
		android:layout_height="100.0dip">
		
        <TextView 
			android:textSize="26.0sp"
			android:textColor="@color/black"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" 
			android:text="@string/app_name"
			android:shadowColor="#ff666666" 
			android:shadowDx="4.0"
			android:shadowDy="4.0" 
			android:shadowRadius="3.0" />
		
    </LinearLayout>
	
    <ScrollView
		android:layout_width="fill_parent"
		android:layout_height="fill_parent">
		
        <LinearLayout
			android:orientation="vertical" 
			android:layout_width="fill_parent" 
			android:layout_height="fill_parent">
			
            <View 
				android:background="@color/linear_text" 
				android:layout_width="fill_parent" 
				android:layout_height="1.0dip"/>
			
            <LinearLayout 
				android:orientation="horizontal" 
				android:id="@+id/leftlayoutfloatwindow"
				android:layout_width="fill_parent" 
				android:layout_height="46.0dip">
				
                <TextView
					android:textSize="24.0sp" 
					android:textColor="@color/black" 
					android:layout_gravity="center_vertical"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content" 
					android:layout_marginLeft="3.0dip"
					android:text="悬浮窗" />
				
            </LinearLayout>
			
            <View
				android:background="@color/linear_text" 
				android:layout_width="fill_parent" 
				android:layout_height="1.0dip"
				android:layout_marginLeft="8.0dip" 
				android:layout_marginRight="8.0dip" />
			
            <LinearLayout 
				android:orientation="horizontal" 
				android:id="@+id/leftlayoutlx"
				android:layout_width="fill_parent"
				android:layout_height="46.0dip">
				
                <TextView 
					android:textSize="24.0sp" 
					android:textColor="@color/black"
					android:layout_gravity="center_vertical" 
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3.0dip" 
					android:text="联系" />
				
            </LinearLayout>
			
			<View 
				android:background="@color/linear_text" 
				android:layout_width="fill_parent" 
				android:layout_height="1.0dip" 
				android:layout_marginLeft="8.0dip" 
				android:layout_marginRight="8.0dip" />

            <LinearLayout
				android:orientation="horizontal" 
				android:id="@+id/leftlayoutfeedback" 
				android:layout_width="fill_parent"
				android:layout_height="46.0dip">

                <TextView 
					android:textSize="24.0sp" 
					android:textColor="@color/black" 
					android:layout_gravity="center_vertical" 
					android:layout_width="fill_parent" 
					android:layout_height="wrap_content"
					android:layout_marginLeft="3.0dip"
					android:text="反馈" />

            </LinearLayout>
			
            <View 
				android:background="@color/linear_text" 
				android:layout_width="fill_parent" 
				android:layout_height="1.0dip" 
				android:layout_marginLeft="8.0dip" 
				android:layout_marginRight="8.0dip" />
			
            <LinearLayout
				android:orientation="horizontal" 
				android:id="@+id/leftlayoutshare" 
				android:layout_width="fill_parent"
				android:layout_height="46.0dip">
				
                <TextView 
					android:textSize="24.0sp" 
					android:textColor="@color/black" 
					android:layout_gravity="center_vertical" 
					android:layout_width="fill_parent" 
					android:layout_height="wrap_content"
					android:layout_marginLeft="3.0dip"
					android:text="分享" />
				
            </LinearLayout>
			
            <View
				android:background="@color/linear_text"
				android:layout_width="fill_parent" 
				android:layout_height="1.0dip"
				android:layout_marginLeft="8.0dip" 
				android:layout_marginRight="8.0dip" />
			
            <LinearLayout
				android:orientation="horizontal" 
				android:id="@+id/leftlayoutpact"
				android:layout_width="fill_parent"
				android:layout_height="46.0dip">
				
                <TextView 
					android:textSize="24.0sp" 
					android:textColor="@color/black"
					android:layout_gravity="center_vertical" 
					android:layout_width="fill_parent"
					android:layout_height="wrap_content" 
					android:layout_marginLeft="3.0dip"
					android:text="协议" />
				
            </LinearLayout>
			
            <View 
				android:background="@color/linear_text" 
				android:layout_width="fill_parent" 
				android:layout_height="1.0dip" 
				android:layout_marginLeft="8.0dip"
				android:layout_marginRight="8.0dip"/>
			
            <LinearLayout
				android:orientation="horizontal" 
				android:id="@+id/leftlayoutupdate" 
				android:layout_width="fill_parent" 
				android:layout_height="46.0dip">
				
                <TextView 
					android:textSize="24.0sp" 
					android:textColor="@color/black"
					android:layout_gravity="center_vertical"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="3.0dip" 
					android:text="更新" />
				
            </LinearLayout>
			
            <View 
				android:background="@color/linear_text"
				android:layout_width="fill_parent" 
				android:layout_height="1.0dip"
				android:layout_marginLeft="8.0dip"
				android:layout_marginRight="8.0dip" />
			
            <LinearLayout 
				android:orientation="horizontal"
				android:id="@+id/leftlayoutmore" 
				android:layout_width="fill_parent" 
				android:layout_height="46.0dip">
				
                <TextView 
					android:textSize="24.0sp" 
					android:textColor="@color/black" 
					android:layout_gravity="center_vertical"
					android:layout_width="fill_parent" 
					android:layout_height="wrap_content" 
					android:layout_marginLeft="3.0dip"
					android:text="计划" />
				
            </LinearLayout>
			
            <View 
				android:background="@color/linear_text"
				android:layout_width="fill_parent" 
				android:layout_height="1.0dip"/>
			
        </LinearLayout>
		
    </ScrollView>
	
</LinearLayout>
